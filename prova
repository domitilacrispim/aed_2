package jogo;

/**
 *
 * @author a11611BCC013
 */
public class Jogador {
    String nome;
    int pontos;
    public void cria(String s){
        nome=s;
        pontos=0;
    }
    public String retornanome(){
        return nome;
    }
    public void addponto (int a){
        pontos=pontos+a;
    }
    public int retornaponto (){
        return pontos;
    }
}

Jogo.java

